{
    "name": "tafern/sidebeep-service-base",
    "description": "Base PHP Application for Services",
    "license": "proprietary",
    "authors": [
        {
            "name": "Irvan Setiawan",
            "email": "irvan.setiawan@tafern.com"
        },
        {
            "name": "Rudi Hermanto",
            "email": "rudi.hermanto@tafern.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sidebeep\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:tafern/sidebeep-service.git"
        }
